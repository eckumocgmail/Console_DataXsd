<?xml version="1.0" encoding="utf-8"?>
<xs:schema targetNamespace="http://schemas.microsoft.com/developer/msbuild/2003" xmlns:xs="http://www.w3.org/2001/XMLSchema" xmlns:msb="http://schemas.microsoft.com/developer/msbuild/2003" elementFormDefault="qualified">

    <!-- =================== INCLUDE COMMON SCHEMA =========================== -->
    <xs:include schemaLocation="Microsoft.Build.Core.xsd"/>
    
    <!-- ======================== ITEMS =====================================-->
    <!-- Possible Types include SimpleItemType (no meta-data subelements), GenericItemType (any meta-data), or something more specific.-->
    <xs:element name="Reference" substitutionGroup="msb:Item">
        <xs:annotation>
            <xs:documentation><!-- _locID_text="Reference" _locComment="" -->Ссылка на сборку</xs:documentation>
        </xs:annotation>
        <xs:complexType>
            <xs:complexContent>
                <xs:extension base="msb:SimpleItemType">
                    <xs:sequence minOccurs="0" maxOccurs="unbounded">
                        <xs:choice>
                            <xs:element name="HintPath">
                                <xs:annotation>
                                    <xs:documentation><!-- _locID_text="Reference_HintPath" _locComment="" -->Относительный или абсолютный путь к сборке (необязательный)</xs:documentation>
                                </xs:annotation>
                            </xs:element>
                            <xs:element name="Name">
                                <xs:annotation>
                                    <xs:documentation><!-- _locID_text="Reference_Name" _locComment="" -->Отображаемое понятное имя (необязательный)</xs:documentation>
                                </xs:annotation>
                            </xs:element>
                            <xs:element name="FusionName">
                                <xs:annotation>
                                    <xs:documentation><!-- _locID_text="Reference_FusionName" _locComment="" -->Fusion-имя сборки (необязательный)</xs:documentation>
                                </xs:annotation>
                            </xs:element>                            
                            <xs:element name="SpecificVersion">
                                <xs:annotation>
                                    <xs:documentation><!-- _locID_text="Reference_SpecificVersion" _locComment="" -->Должна быть создана ссылка только на версию в fusion-имени или нет (необязательный, логический)</xs:documentation>
                                </xs:annotation>
                            </xs:element>
                            <xs:element name="Aliases">
                                <xs:annotation>
                                    <xs:documentation><!-- _locID_text="Reference_Aliases" _locComment="" -->Псевдонимы для ссылки (необязательный)</xs:documentation>
                                </xs:annotation>
                            </xs:element>
                            <xs:element name="Private">
                                <xs:annotation>
                                    <xs:documentation><!-- _locID_text="Reference_Private" _locComment="" -->Должна ссылка копироваться в выходную папку или нет (необязательный, логический)</xs:documentation>
                                </xs:annotation>
                            </xs:element>
                          <xs:element name="EmbedInteropTypes" type="msb:boolean">
                            <xs:annotation>
                              <xs:documentation><!-- _locID_text="Reference_EmbedInteropTypes" _locComment="" -->Указывает, требуется ли внедрить типы из этой ссылки в конечную сборку - только сборки взаимодействия (необязательный, логический)</xs:documentation>
                            </xs:annotation>
                          </xs:element>
                          <xs:element name="RequiredTargetFramework">
                                <xs:annotation>
                                    <xs:documentation><!-- _locID_text="Reference_RequiredTargetFramework" _locComment="" -->Минимальная используемая версия .Net Framework для ссылки на данную сборку</xs:documentation>
                                </xs:annotation>
                            </xs:element>                                
                        </xs:choice>
                    </xs:sequence>
                    <!-- redefine Include just to give a specific description -->
                    <xs:attribute name="Include" type="xs:string" use="optional">
                        <xs:annotation>
                            <xs:documentation><!-- _locID_text="Reference_Include" _locComment="" -->Имя сборки или имя файла сборки</xs:documentation>
                        </xs:annotation>
                    </xs:attribute>
                </xs:extension>
            </xs:complexContent>
        </xs:complexType>
    </xs:element>
    <xs:element name="COMReference" substitutionGroup="msb:Item">
        <xs:annotation>
            <xs:documentation><!-- _locID_text="COMReference" _locComment="" -->Ссылка на COM-компонент</xs:documentation>
        </xs:annotation>
        <xs:complexType>
            <xs:complexContent>
                <xs:extension base="msb:SimpleItemType">
                    <xs:sequence minOccurs="0" maxOccurs="unbounded">
                        <xs:choice>
                            <xs:element name="Name">
                                <xs:annotation>
                                    <xs:documentation><!-- _locID_text="COMReference_Name" _locComment="" -->Отображаемое понятное имя (необязательный)</xs:documentation>
                                </xs:annotation>
                            </xs:element>
                            <xs:element name="Guid">
                                <xs:annotation>
                                    <xs:documentation><!-- _locID_text="COMReference_Guid" _locComment="" -->Идентификатор GUID в формате {00000000-0000-0000-0000-000000000000}</xs:documentation>
                                </xs:annotation>
                            </xs:element>
                            <xs:element name="VersionMajor">
                                <xs:annotation>
                                    <xs:documentation><!-- _locID_text="COMReference_VersionMajor" _locComment="" -->Старшая часть номера версии</xs:documentation>
                                </xs:annotation>
                            </xs:element>
                            <xs:element name="VersionMinor">
                                <xs:annotation>
                                    <xs:documentation><!-- _locID_text="COMReference_VersionMinor" _locComment="" -->Младшая часть номера версии</xs:documentation>
                                </xs:annotation>
                            </xs:element>
                            <xs:element name="Lcid">
                                <xs:annotation>
                                    <xs:documentation><!-- _locID_text="COMReference_Lcid" _locComment="" -->Идентификатор языка</xs:documentation>
                                </xs:annotation>
                            </xs:element>
                            <xs:element name="WrapperTool">
                                <xs:annotation>
                                    <xs:documentation><!-- _locID_text="COMReference_WrapperTool" _locComment="" -->Инструмент обертки, например tlbimp</xs:documentation>
                                </xs:annotation>
                            </xs:element>
                            <xs:element name="Isolated">
                                <xs:annotation>
                                    <xs:documentation><!-- _locID_text="COMReference_Isolated" _locComment="" -->Является изолированным или нет (логический)</xs:documentation>
                                </xs:annotation>
                            </xs:element>
                          <xs:element name="EmbedInteropTypes" type="msb:boolean">
                            <xs:annotation>
                              <xs:documentation><!-- _locID_text="COMReference_EmbedInteropTypes" _locComment="" -->Указывает, требуется ли внедрить типы из этой ссылки в конечную сборку - только сборки взаимодействия (необязательный, логический)</xs:documentation>
                            </xs:annotation>
                          </xs:element>
                        </xs:choice>
                    </xs:sequence>
                    <!-- redefine Include just to give a specific description -->
                    <xs:attribute name="Include" type="xs:string" use="optional">
                        <xs:annotation>
                            <xs:documentation><!-- _locID_text="COMReference_Include" _locComment="" -->Имя COM-компонента</xs:documentation>
                        </xs:annotation>
                    </xs:attribute>
                </xs:extension>
            </xs:complexContent>
        </xs:complexType>
    </xs:element>
    <xs:element name="COMFileReference" substitutionGroup="msb:Item">
        <xs:complexType>
            <xs:complexContent>
                <xs:extension base="msb:SimpleItemType">
                    <xs:sequence minOccurs="0" maxOccurs="unbounded">
                        <xs:choice>
                            <xs:element name="WrapperTool"/>
                        </xs:choice>
                    </xs:sequence>
        </xs:extension>
      </xs:complexContent>
    </xs:complexType>
  </xs:element>
  <xs:element name="Xdcmake" substitutionGroup="msb:Item">
    <xs:complexType>
      <xs:complexContent>
        <xs:extension base="msb:SimpleItemType">
          <xs:sequence minOccurs="0" maxOccurs="unbounded">
            <xs:choice>
              <xs:element name="SuppressStartupBanner"/>
              <xs:element name="AdditionalDocumentFile"/>
              <xs:element name="DocumentLibraryDependencies"/>
            </xs:choice>
          </xs:sequence>
        </xs:extension>
      </xs:complexContent>
    </xs:complexType>
  </xs:element>
  <xs:element name="Bscmake" substitutionGroup="msb:Item">
    <xs:complexType>
      <xs:complexContent>
        <xs:extension base="msb:SimpleItemType">
          <xs:sequence minOccurs="0" maxOccurs="unbounded">
            <xs:choice>
              <xs:element name="PreserveSBR"/>
              <xs:element name="AdditionalOptions"/>
              <xs:element name="SuppressStartupBanner"/>
              <xs:element name="OutputFile"/>
            </xs:choice>
          </xs:sequence>
        </xs:extension>
      </xs:complexContent>
    </xs:complexType>
  </xs:element>
  <xs:element name="ClCompile" substitutionGroup="msb:Item">
    <xs:complexType>
      <xs:complexContent>
        <xs:extension base="msb:SimpleItemType">
          <xs:choice minOccurs="0" maxOccurs="unbounded">
            <xs:element name="PrecompiledHeader" minOccurs="0">
              <xs:complexType>
                <xs:simpleContent>
                  <xs:extension base="xs:string">
                    <xs:attribute name="Condition" use="optional"></xs:attribute>
                </xs:extension>
                </xs:simpleContent>
              </xs:complexType>
            </xs:element>
            <xs:element name="AdditionalIncludeDirectories"/>
            <xs:element name="AdditionalUsingDirectories"/>
            <xs:element name="CompileAsManaged">
              <xs:complexType>
                <xs:simpleContent>
                  <xs:extension base="xs:string">
                    <xs:attribute name="Condition" use="optional"></xs:attribute>
                  </xs:extension>
                </xs:simpleContent>
              </xs:complexType>
            </xs:element>
            <xs:element name="ErrorReporting"/>
            <xs:element name="WarningLevel"/>
            <xs:element name="MinimalRebuild"/>
            <xs:element name="DebugInformationFormat"/>
            <xs:element name="PreprocessorDefinitions"/>
            <xs:element name="Optimization"/>
            <xs:element name="BasicRuntimeChecks"/>
            <xs:element name="RuntimeLibrary"/>
            <xs:element name="FunctionLevelLinking"/>
            <xs:element name="FloatingPointModel "/>
            <xs:element name="IntrinsicFunctions"/>
            <xs:element name="PrecompiledHeaderFile"/>
            <xs:element name="MultiProcessorCompilation"/>
            <xs:element name="UseUnicodeForAssemblerListing"/>
            <xs:element name="UndefinePreprocessorDefinitions"/>
            <xs:element name="StringPooling"/>
            <xs:element name="BrowseInformation"/>
            <xs:element name="FloatingPointExceptions"/>
            <xs:element name="CreateHotpatchableImage"/>
            <xs:element name="RuntimeTypeInfo"/>
            <xs:element name="OpenMPSupport"/>
            <xs:element name="CallingConvention"/>
            <xs:element name="DisableSpecificWarnings"/>
            <xs:element name="ForcedIncludeFiles"/>
            <xs:element name="ForcedUsingFiles"/>
            <xs:element name="ShowIncludes"/>
            <xs:element name="UseFullPaths"/>
            <xs:element name="OmitDefaultLibName"/>
           <xs:element name="TreatSpecificWarningsAsErrors"/>
         </xs:choice>
       </xs:extension>
      </xs:complexContent>
     </xs:complexType>
   </xs:element>
  <xs:element name="ClInclude" substitutionGroup="msb:Item">
    <xs:complexType>
      <xs:complexContent>
        <xs:extension base="msb:SimpleItemType">
          <xs:choice minOccurs="0" maxOccurs="unbounded">
            <xs:element name="AdditionalIncludeDirectories" minOccurs="0"/>
          </xs:choice>
        </xs:extension>
      </xs:complexContent>
    </xs:complexType>
  </xs:element>
  <xs:element name="Midl" substitutionGroup="msb:Item">
    <xs:complexType>
      <xs:complexContent>
        <xs:extension base="msb:SimpleItemType">
          <xs:choice minOccurs="0" maxOccurs="unbounded">
            <xs:element name="MkTypLibCompatible"/>
            <xs:element name="ValidateAllParameters" type="msb:boolean"/>
            <xs:element name="PreprocessorDefinitions"/>
            <xs:element name="TypeLibraryName"/>
            <xs:element name="ErrorCheckRefPointers"/>
            <xs:element name="ErrorCheckStubData"/>
            <xs:element name="ErrorCheckBounds"/>
            <xs:element name="HeaderFileName"/>
            <xs:element name="ErrorCheckEnumRange"/>
            <xs:element name="ErrorCheckAllocations"/>
            <xs:element name="WarnAsError"/>
            <xs:element name="GenerateTypeLibrary"/>
            <xs:element name="AdditionalIncludeDirectories"/>
            <xs:element name="IgnoreStandardIncludePath"/>
            <xs:element name="SuppressStartupBanner"/>
            <xs:element name="DefaultCharType"/>
            <xs:element name="TargetEnvironment"/>
            <xs:element name="GenerateStublessProxies"/>
            <xs:element name="SuppressCompilerWarnings"/>
            <xs:element name="ApplicationConfigurationMode"/>
            <xs:element name="LocaleID"/>
            <xs:element name="OutputDirectory"/>
            <xs:element name="DllDataFileName"/>
            <xs:element name="InterfaceIdentifierFileName"/>
            <xs:element name="ProxyFileName"/>
            <xs:element name="GenerateClientFiles"/>
            <xs:element name="GenerateServerFiles"/>
            <xs:element name="ClientStubFile"/>
            <xs:element name="ServerStubFile"/>
            <xs:element name="TypeLibFormat"/>
            <xs:element name="CPreprocessOptions"/>
            <xs:element name="UndefinePreprocessorDefinitions"/>
            <xs:element name="EnableErrorChecks"/>
            <xs:element name="RedirectOutputAndErrors"/>
            <xs:element name="AdditionalOptions"/>
            <xs:element name="StructMemberAlignment"/>
          </xs:choice>
        </xs:extension>
      </xs:complexContent>
    </xs:complexType>
  </xs:element>
  <xs:element name="ResourceCompile" substitutionGroup="msb:Item">
    <xs:complexType>
      <xs:complexContent>
        <xs:extension base="msb:SimpleItemType">
          <xs:choice minOccurs="0" maxOccurs="unbounded">
            <xs:element name="Culture"/>
            <xs:element name="PreprocessorDefinitions"/>
            <xs:element name="AdditionalIncludeDirectories"/>
          </xs:choice>
        </xs:extension>
      </xs:complexContent>
    </xs:complexType>
  </xs:element>
  <xs:element name="PreLinkEvent" substitutionGroup="msb:Item">
    <xs:complexType>
      <xs:complexContent>
        <xs:extension base="msb:SimpleItemType">
          <xs:choice minOccurs="0" maxOccurs="unbounded">
            <xs:element name="Message"/>
            <xs:element name="Command"/>
          </xs:choice>
        </xs:extension>
      </xs:complexContent>
    </xs:complexType>
  </xs:element>
  <xs:element name="CustomBuildStep" substitutionGroup="msb:Item">
    <xs:complexType>
      <xs:complexContent>
        <xs:extension base="msb:SimpleItemType">
          <xs:choice minOccurs="0" maxOccurs="unbounded">
            <xs:element name="Outputs"/>
            <xs:element name="Message"/>
            <xs:element name="Inputs"/>
            <xs:element name="Command"/>
          </xs:choice>
        </xs:extension>
      </xs:complexContent>
    </xs:complexType>
  </xs:element>
  <xs:element name="Manifest" substitutionGroup="msb:Item">
    <xs:complexType>
      <xs:complexContent>
        <xs:extension base="msb:SimpleItemType">
          <xs:choice minOccurs="0" maxOccurs="unbounded">
            <xs:element name="AssemblyIdentity"/>
            <xs:element name="AdditionalManifestFiles"/>
            <xs:element name="InputResourceManifests"/>
            <xs:element name="EnableDPIAwareness"/>
            <xs:element name="TypeLibraryFile"/>
            <xs:element name="OutputManifestFile"/>
            <xs:element name="SuppressStartupBanner"/>
            <xs:element name="VerboseOutput"/>
            <xs:element name="ResourceOutputFileName"/>
            <xs:element name="GenerateCatalogFiles"/>
            <xs:element name="DependencyInformationFile"/>
            <xs:element name="ManifestFromManagedAssembly"/>
            <xs:element name="SuppressDependencyElement"/>
            <xs:element name="GenerateCategoryTags"/>
            <xs:element name="RegistrarScriptFile"/>
            <xs:element name="ComponentFileName"/>
            <xs:element name="ReplacementsFile"/>
            <xs:element name="UpdateFileHashesSearchPath"/>
            <xs:element name="AdditionalOptions"/>
            <xs:element name="OutputResourceManifests"/>
          </xs:choice>
        </xs:extension>
      </xs:complexContent>
    </xs:complexType>
  </xs:element>
  <xs:element name="ProjectConfiguration" substitutionGroup="msb:Item">
    <xs:complexType>
      <xs:complexContent>
        <xs:extension base="msb:SimpleItemType">
          <xs:sequence minOccurs="1" maxOccurs="1">
            <xs:element name="Configuration" maxOccurs="1"/>
            <xs:element name="Platform" maxOccurs="1"/>
          </xs:sequence>
        </xs:extension>
      </xs:complexContent>
    </xs:complexType>
  </xs:element>
    <xs:element name="NativeReference" substitutionGroup="msb:Item">
        <xs:annotation>
            <xs:documentation><!-- _locID_text="NativeReference" _locComment="" -->Ссылка на машинный файл манифеста или на файл, который содержит машинный манифест</xs:documentation>
        </xs:annotation>        
        <xs:complexType>
            <xs:complexContent>
                <xs:extension base="msb:SimpleItemType">
                    <xs:sequence minOccurs="0" maxOccurs="unbounded">
                        <xs:choice>
                            <xs:element name="Name">
                                <xs:annotation>
                                    <xs:documentation><!-- _locID_text="NativeReference_Name" _locComment="" -->Базовое имя файла манифеста</xs:documentation>
                                </xs:annotation>
                            </xs:element>                                
                            <xs:element name="HintPath">
                                <xs:annotation>
                                    <xs:documentation><!-- _locID_text="NativeReference_HintPath" _locComment="" -->Относительный путь к файлу манифеста</xs:documentation>
                                </xs:annotation>
                            </xs:element>
                        </xs:choice>
                    </xs:sequence>
                    <!-- redefine Include just to give a specific description -->
                    <xs:attribute name="Include" type="xs:string" use="optional">
                        <xs:annotation>
                            <xs:documentation><!-- _locID_text="NativeReference_Include" _locComment="" -->Полное имя ссылки</xs:documentation>
                        </xs:annotation>
                    </xs:attribute>                    
                </xs:extension>
            </xs:complexContent>
        </xs:complexType>
    </xs:element>
    <xs:element name="ProjectReference" substitutionGroup="msb:Item">
        <xs:annotation>
            <xs:documentation><!-- _locID_text="ProjectReference" _locComment="" -->Ссылка на другой проект</xs:documentation>
        </xs:annotation>
        <xs:complexType>
            <xs:complexContent>
                <xs:extension base="msb:SimpleItemType">
                    <xs:sequence minOccurs="0" maxOccurs="unbounded">
                        <xs:choice>
                            <xs:element name="Name">
                                <xs:annotation>
                                    <xs:documentation><!-- _locID_text="ProjectReference_Name" _locComment="" -->Отображаемое понятное имя (необязательный)</xs:documentation>
                                </xs:annotation>
                            </xs:element>
                            <xs:element name="Project">
                                <xs:annotation>
                                    <xs:documentation><!-- _locID_text="ProjectReference_Project" _locComment="" -->Идентификатор GUID проекта в формате {00000000-0000-0000-0000-000000000000}</xs:documentation>
                                </xs:annotation>
                            </xs:element>
                            <xs:element name="ReferenceOutputAssembly" type="msb:boolean">
                                <xs:annotation>
                                    <xs:documentation><!-- _locID_text="ProjectReference_Reference" _locComment="" -->Логическое значение, определяющее, должны ли передаваться в компилятор выходные данные проекта, на который указывает ссылка. Значение по умолчанию - true.</xs:documentation>
                                </xs:annotation>
                            </xs:element> 
                            <xs:element name="SpecificVersion">
                                <xs:annotation>
                                    <xs:documentation><!-- _locID_text="Reference_SpecificVersion" _locComment="" -->Должна ли использоваться точная версия сборки.</xs:documentation>
                                </xs:annotation>
                            </xs:element>                            
                            <xs:element name="Targets">
                                <xs:annotation>
                                    <xs:documentation><!-- _locID_text="ProjectReference_Targets" _locComment="" -->Список задач (разделитель - точкой с запятой) в связанных проектах, которые следует построить. В качестве значения по умолчанию используется значение $(ProjectReferenceBuildTargets), которое в свою очередь по умолчанию пусто, что соответствует задачам по умолчанию.</xs:documentation>
                                </xs:annotation>
                            </xs:element>
                            <xs:element name="OutputItemType">
                             <xs:annotation>
                               <xs:documentation>
                                 <!-- _locID_text="ProjectReference_OutputItemType" _locComment="" -->Тип элемента для размещения конечных выходных данных. Значение по умолчанию - пустое. Если для метаданных ссылки установлено значение "true" (по умолчанию), конечные выходные данные станут ссылками для компилятора.
                </xs:documentation>
                              </xs:annotation>
                            </xs:element>
                            <xs:element name="Package"/>
                            <xs:element name="EmbedInteropTypes" type="msb:boolean">
                                <xs:annotation>
                                    <xs:documentation><!-- _locID_text="ProjectReference_EmbedInteropTypes" _locComment="" -->Указывает, требуется ли внедрить типы из этой ссылки в конечную сборку - только сборки взаимодействия (необязательный, логический)</xs:documentation>
                                </xs:annotation>
                            </xs:element>
                        </xs:choice>
                    </xs:sequence>
                    <!-- redefine Include just to give a specific description -->
                    <xs:attribute name="Include" type="xs:string" use="optional">
                        <xs:annotation>
                            <xs:documentation><!-- _locID_text="ProjectReference_Package" _locComment="" -->Путь к файлу проекта</xs:documentation>
                        </xs:annotation>
                    </xs:attribute>
                </xs:extension>
            </xs:complexContent>
        </xs:complexType>
    </xs:element>    
    <xs:element name="Compile" substitutionGroup="msb:Item">
        <xs:annotation>
            <xs:documentation><!-- _locID_text="Compile" _locComment="" -->Исходные файлы для компилятора</xs:documentation>
        </xs:annotation>        
        <xs:complexType>           
            <xs:complexContent>
                <xs:extension base="msb:SimpleItemType">
                    <xs:sequence minOccurs="0" maxOccurs="unbounded">                       
                        <xs:choice>
                            <xs:element name="SubType"/>
                            <xs:element name="DependentUpon"/>
                            <xs:element name="AutoGen">
                                <xs:annotation>
                                    <xs:documentation><!-- _locID_text="Compile_AutoGen" _locComment="" -->Был файл создан из другого файла или нет (логический)</xs:documentation>
                                </xs:annotation>
                            </xs:element>                                 
                            <xs:element name="DesignTime"/>
                            <xs:element name="Link">
                                <xs:annotation>
                                    <xs:documentation><!-- _locID_text="Compile_Link" _locComment="" -->Символический путь в пределах проекта для отображения в случае, если файл физически расположен вне иерархии файла проекта (необязательный)</xs:documentation>
                                </xs:annotation>
                            </xs:element>
                            <xs:element name="DesignTimeSharedInput"/>
                            <xs:element name="Visible">
                                <xs:annotation>
                                    <xs:documentation><!-- _locID_text="Compile_InProject" _locComment="" -->Отображать в интерфейсе пользователя (необязательный, логический)</xs:documentation>
                                </xs:annotation>
                            </xs:element>                                
                            <xs:element name="CopyToOutputDirectory"/>
                            <xs:element name="VBMyExtensionTemplateID"/>
                        </xs:choice>
                    </xs:sequence>
                    <!-- redefine Include just to give a specific description -->
                    <xs:attribute name="Include" type="xs:string" use="optional">
                        <xs:annotation>
                            <xs:documentation><!-- _locID_text="Compile_Include" _locComment="" -->Список исходных файлов (разделитель - точка с запятой, разрешены знаки подстановки)</xs:documentation>
                        </xs:annotation>
                    </xs:attribute>                    
                </xs:extension>
            </xs:complexContent>
        </xs:complexType>
    </xs:element>
    <xs:element name="EmbeddedResource" substitutionGroup="msb:Item">
        <xs:annotation>
            <xs:documentation><!-- _locID_text="EmbeddedResource" _locComment="" -->Ресурсы для внедрения в созданную сборку</xs:documentation>
        </xs:annotation>        
        <xs:complexType>
            <xs:complexContent>
                <xs:extension base="msb:SimpleItemType">
                    <xs:sequence minOccurs="0" maxOccurs="unbounded">
                        <xs:choice>
                            <xs:element name="DependentUpon"/>
                            <xs:element name="Generator">
                                <xs:annotation>
                                    <xs:documentation><!-- _locID_text="EmbeddedResource_Generator" _locComment="" -->Имя произвольного генератора файлов, который выполняется для этого элемента</xs:documentation>
                                </xs:annotation>
                            </xs:element>                               
